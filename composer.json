{
    "name": "pwweb/laravel-core",
    "description": "Additional core functionalities for Laravel",
    "license": "MIT",
    "authors": [{
        "name": "Frank Pillukeit",
        "email": "clients@pw-websolutions.com",
        "homepage": "https://www.pw-websolutions.com"
    }],
    "homepage": "https://github.com/pwweb/core",
    "keywords": ["library", "laravel", "core", "traits"],
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^8.0",
        "laravel/socialite": "^5.0",
        "laracasts/flash": "^3.0",
        "spatie/enum": "^2.3",
        "spatie/laravel-enum": "^1.6",
        "spatie/laravel-medialibrary": "^8.0",
        "spatie/laravel-permission": "^3.16",
        "pwweb/localisation": "*@dev",
        "kalnoy/nestedset": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "fzaninotto/faker": "^1.9.1",
        "squizlabs/php_codesniffer": "3.*",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^0.12.42",
        "orchestra/testbench": "^6.0",
        "psalm/plugin-laravel": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "PWWEB\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PWWEB\\Core\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PWWEB\\Core\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "PWWEB\\Core\\Facades\\Core"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}